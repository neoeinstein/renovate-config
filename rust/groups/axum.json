{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Axum ecosystem crates",
  "packageRules": [
    {
      "groupName": "axum crates",
      "matchDatasources": [
        "crate"
      ],
      "matchManagers": [
        "cargo"
      ],
      "matchSourceUrls": [
        "https://github.com/tokio-rs/axum"
      ]
    }
  ]
}
