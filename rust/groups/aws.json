{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "groupName": "AWS SDK crates",
      "matchDatasources": [
        "crate"
      ],
      "matchManagers": [
        "cargo"
      ],
      "matchSourceUrls": [
        "https://github.com/awslabs/aws-sdk-rust",
        "https://github.com/smithy-lang/smithy-rs"
      ]
    }
  ]
}
